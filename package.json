{
  "name": "cavatina",
  "version": "1.0.0",
  "description": "A minimal & mobile-first theme. The best choice for people who want to introduce their projects.",
  "author": "VitaThemes",
  "license": "GPL-2.0-or-later",
  "keywords": [
    "WordPress",
    "Theme"
  ],
  "homepage": "https://demo.vitathemes.com/cavatina/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vitathemes/cavatina"
  },
  "bugs": {
    "url": "https://github.com/vitathemes/cavatina/issues"
  },
  "devDependencies": {
    "@wordpress/scripts": "^12.1.0",
    "browser-sync": "^2.26.13",
    "dir-archiver": "^1.1.1",
    "gulp": "^4.0.2",
    "gulp-imagemin": "^7.1.0",
    "gulp-uglify": "^3.0.2",
    "node-sass": "^4.14.1",
    "pump": "^3.0.0",
    "rtlcss": "^2.5.0"
  },
  "rtlcssConfig": {
    "options": {
      "autoRename": false,
      "autoRenameStrict": false,
      "blacklist": {},
      "clean": true,
      "greedy": false,
      "processUrls": false,
      "stringMap": []
    },
    "plugins": [],
    "map": false
  },
  "scripts": {
    "watch": "node-sass sass/ -o ./ --source-map true --output-style expanded --indent-type tab --indent-width 1 -w",
    "compile:css": "node-sass sass/ -o ./ && stylelint '*.css' --fix || true && stylelint '*.css' --fix",
    "compile:rtl": "rtlcss style.css style-rtl.css",
    "lint:scss": "wp-scripts lint-style 'sass/**/*.scss'",
    "lint:js": "wp-scripts lint-js 'js/*.js'",
    "bundle": "dir-archiver --src . --dest ../_s.zip --exclude .DS_Store .stylelintrc.json .eslintrc .git .gitattributes .github .gitignore README.md composer.json composer.lock node_modules vendor package-lock.json package.json .travis.yml phpcs.xml.dist sass style.css.map"
  },
  "dependencies": {
    "flickity": "^2.2.2",
    "flickity-sync": "^2.0.0",
    "gulp-clean-css": "^4.3.0",
    "gulp-concat": "^2.6.1",
    "gulp-sass": "^4.1.0",
    "jquery": "^3.5.1",
    "simplebar": "^5.3.0",
    "swiper": "^6.4.1",
    "uglify-js": "^3.12.1",
    "vanilla-lazyload": "^17.3.1"
  }
}
